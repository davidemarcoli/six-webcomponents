{
  "name": "vitepress-docs",
  "version": "1.0.0",
  "description": "Six Webcomponents documentation",
  "type": "module",
  "scripts": {
    "docs:dev": "vitepress dev",
    "docs:build": "npm run setup && vitepress build",
    "docs:preview": "vitepress preview",
    "setup": "npm run setup:copy && npm run setup:components",
    "setup:copy": "node scripts/copy.js",
    "setup:components": "node scripts/components.js",
    "setup:version": "node scripts/version.js",
    "clean": "rimraf src/.vitepress/dist src/.vitepress/lib src/components/* src/.vitepress/components/* src/.vitepress/public/components/assets src/.vitepress/public/components/*.js src/CHANGELOG.md"
  },
  "author": "Colin Schwarz",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^8.45.0",
    "fs-extra": "^11.1.1",
    "git-rev-sync": "^3.0.2",
    "node-html-parser": "^6.1.5",
    "rimraf": "^5.0.1",
    "simple-node-logger": "^21.8.12",
    "turndown": "^7.1.2",
    "vitepress": "^1.0.0-beta.5"
  }
}
